Program _Ap;

Start Var Int_4 _Aa, _Bb, _Cc, _Mx;

   Scan (_Aa);
   Scan (_Bb);
   Scan (_Cc);

   If _Aa Ge _Bb And _Aa Ge _Cc {
       _Aa -> _Mx;
     } Else if _Bb Ge _Aa And _Bb Ge _Cc {
       _Bb -> _Mx;
     } Else {
       _Cc -> _Mx;
     }
     Print (_Mx);
   
     If ( _Aa Eg _Bb ) And ( _Aa Eg _Cc ) And ( _Bb Eg _Cc ) {
       Print (1);
     } Else {
       Print (0);
     }
       
     If not ( _Aa Ge 0 ) Or Not ( _Bb Ge 0 ) Or Not ( _Cc Ge 0 ) {
       Print (-1);
     } Else {
       Print (0);
     }
   
     If _Aa Ge ( _Bb ++ _Cc ) {
       Print (10);
     } Else {
       Print (0);
     }
   
Finish
